
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js" integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js" integrity="sha384-mQ93GR66B00ZXjt0YO5KlohRA5SY2XofN4zfuZxLkoj1gXtW8ANNCe9d5Y3eG5eD" crossorigin="anonymous"></script>





<h1 style="text-align: center; margin-top: 30px;">All Parties</h1>
<div class="tableDiv">
  <table class="table table-striped">
    <tbody>
        <%  student.forEach(element => { %>
          <div class="card" style="width: 20 rem;">
            <div class="card-body" style="justify-content: center;">
              <h1 class="card-title"><%=element.name%></h1>
              <h6 class="card-subtitle mb-2 text-muted">Venue:<%=element.venue%></h6>
              <h6>Party Code: <%=element.partycode%></h6>
              <p class="card-text">Date:  <%=element.date%></p>
              <p class="card-text">Time:   <%=element.time%></p>
              <p class="card-text">Budget: <%=element.budget%></p>
               <a href="/teacher/delete/<%= element._id %> "><img style="margin: 5px 5px;" src="https://img.icons8.com/ios-glyphs/30/000000/filled-trash.png"/></a>
               <a href="/add_participant?id=<%=element._id%>" class="btn btn-primary">Add Participant</a>
              <!-- <a href="/teacher/showparticipant/<%=element._id%>" class="btn btn-primary" style="margin-left: 5px;">Show Participants</a>
               <a href="/teacher/knowexpenses/<%=element.partycode%>" class="btn btn-primary">Contributors</a> -->
            </div>
          </div>
        <%}) %>
    </tbody>
  </table>
</div>


  